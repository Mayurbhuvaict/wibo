{% block stock_import_list %}
    <sw-page class="stock-import-list">
        <template slot="content">
            <sw-card class="sw-settings-shipping-detail__condition_container">
                <div class="collection-container">
                    <div style="width:100%;">
                        <h1>{{ $t('stock-import.label.title') }}</h1>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <sw-button id="aepImportButton"
                                       :disabled="false"
                                       variant="primary"
                                       :square="false"
                                       :block="false"
                                       :isLoading="false"
                                       @click="onAepSave">
                                {{ $t('stock-import.label.aepBtn') }}
                            </sw-button>
                        </div>
                        <div class="col-md-4">
                            <sw-button id="amacomImportButton"
                                       :disabled="false"
                                       variant="primary"
                                       :square="false"
                                       :block="false"
                                       :isLoading="false"
                                       @click="onAmacomSave">
                                {{ $t('stock-import.label.amacomBtn') }}
                            </sw-button>
                        </div>
                        <div class="col-md-4">
                            <sw-button id="borettiImportButton"
                                       :disabled="false"
                                       variant="primary"
                                       :square="false"
                                       :block="false"
                                       :isLoading="false"
                                       @click="onBorettiSave">
                                {{ $t('stock-import.label.borettiBtn') }}
                            </sw-button>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-4">
                            <sw-button id="inventumImportButton"
                                       :disabled="false"
                                       variant="primary"
                                       :square="false"
                                       :block="false"
                                       :isLoading="false"
                                       @click="onInventumSave">
                                {{ $t('stock-import.label.inventumBtn') }}
                            </sw-button>
                        </div>
                    </div>
                </div>
            </sw-card>
        </template>
    </sw-page>
{% endblock %}
