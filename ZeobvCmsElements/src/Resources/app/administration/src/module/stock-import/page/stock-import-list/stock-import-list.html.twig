{% block pim_import_list %}
    <sw-page class="pim-import-list">
        <template slot="content">
            <sw-card class="sw-settings-shipping-detail__condition_container">
                <div class="collection-container">
                    <div style="width:100%;">
                        <h1>{{ $t('stock-import.label.title') }}</h1>
                    </div>
                    <sw-button ref="pimMainButton"
                               id="pim_main_button"
                               :disabled="false"
                               variant="primary"
                               :square="false"
                               :block="false"
                               :isLoading="false"
                               @click="onSave">
                        {{ $t('stock-import.label.btn') }}
                    </sw-button>
                    <p id="pim_main_p_counter" style="display:none">
                        {{ PimImportSetting['PimImport.config.mainProductCounter'] }}
                    </p>
                    <p v-if="counter" id="pim_main_status">
                        {{ counter }}{{ ' import product successfully.' }}
                    </p>
                </div>
            </sw-card>
        </template>
    </sw-page>
{% endblock %}
